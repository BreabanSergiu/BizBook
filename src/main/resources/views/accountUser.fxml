<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="650.0" style="-fx-background-color: linear-gradient(#BBD2C5,#536976,#3b4160)" stylesheets="@../css/acountUser.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socialnetwork.controller.AccountUserController">
    <children>
        <TableView fx:id="tableViewAccountUser" layoutX="40.0" layoutY="92.0" prefHeight="338.0" prefWidth="354.0">
            <columns>
                <TableColumn fx:id="tableColumnYourFriends" prefWidth="75.0" text="Your friends">
                    <columns>
                        <TableColumn fx:id="tableColumnFirstName" prefWidth="164.99999618530273" text="First name" />
                        <TableColumn fx:id="tableColumnLastName" prefWidth="203.80003356933594" text="Last name" />
                    </columns>
                </TableColumn>
            </columns>
        </TableView>
        <Button fx:id="buttonAddFriendship" layoutX="437.0" layoutY="92.0" mnemonicParsing="false" onMouseClicked="#addFriendshipRequest" prefHeight="46.0" prefWidth="158.0" text="Add friendship Request" />
        <Button fx:id="buttonDeleteFriendship" layoutX="437.0" layoutY="163.0" mnemonicParsing="false" onMouseClicked="#deleteFriendship" prefHeight="46.0" prefWidth="158.0" text="Delete friendship" />
        <Label fx:id="labelUserName" alignment="CENTER" layoutX="40.0" layoutY="22.0" prefHeight="46.0" prefWidth="354.0" text="NameUser">
            <font>
                <Font size="22.0" />
            </font></Label>

      <Button fx:id="buttonFriendshipRequest" layoutX="437.0" layoutY="238.0" mnemonicParsing="false" onMouseClicked="#viewFriendshipRequest" prefHeight="46.0" prefWidth="158.0" text="View Friendship Request" />
      <Button fx:id="butonMessages" layoutX="437.0" layoutY="310.0" mnemonicParsing="false" onMouseClicked="#viewMessages" prefHeight="46.0" prefWidth="158.0" text="Messages" />
      <Button fx:id="buttonReports" layoutX="437.0" layoutY="384.0" mnemonicParsing="false" onMouseClicked="#showReports" prefHeight="46.0" prefWidth="158.0" text="reports" />

    </children>
</AnchorPane>
